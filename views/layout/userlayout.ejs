          </div> -->
    
          <div class="history_lists">
            <div class="list1">
              <div class="row">
                <h4>Products</h4>
                <a href="#">See all</a>
              </div>
              <table>
                <thead>
                  <tr>
                    <th>Product id</th>
                    <th>Name</th>
                    <th>catagory</th>
                    <th>totalSales</th>
                  </tr>
                </thead>
                    <tbody>
                        <% for (var i = 0; i < products.length ;i++) { %>
                         <tr>
                           <td><%= products[i].id %></td>
                           <td><%= products[i].name %></td>
                           <td><%= products[i].catagory %></td>
                            <td><%= products[i].totalSales %></td>
                         </tr> <% }%>
                    </tbody>
              </table>
            </div>
    
            <div class="list2">
              <div class="row">
                <h4>Documnets</h4>
                <a href="#">Upload</a>
              </div>
              <table>
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Title</th>
                    <th>Type</th>
                    <th>Uplaoded</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td>1</td>
                    <td>CNIC</td>
                    <td>PDF</td>
                    <td>20</td>
                  </tr>
                  <tr>
                    <td>2</td>
                    <td>Passport</td>
                    <td>PDF</td>
                    <td>12</td>
                  </tr>
                  <tr>
                    <td>3</td>
                    <td>Licence</td>
                    <td>PDF</td>
                    <td>9</td>
                  </tr>
                  <tr>
                    <td>4</td>
                    <td>Pic</td>
                    <td>Jpg</td>
                    <td>22</td>
                  </tr> 
                  <tr>
                    <td>5</td>
                    <td>CNIC</td>
                    <td>Jpg</td>
                    <td>22</td>
                  </tr> 
                  <tr>
                    <td>6</td>
                    <td>Docx</td>
                    <td>Word</td>
                    <td>22</td>
                  </tr> 
                </tbody>
              </table>
